/*重置*/
*{padding:0;margin:0}
a, a:visited{color:#555;text-decoration:none;outline:none}
a:hover{color:#3fa9f5}
a{blr:expression(this.onFocus=this.blur())}
ul{list-style:none}
h4,h3,h2,h1{font-weight:normal}
h3{font-size:2.5em;width:100%;text-align:center;color:#aaa;padding:0.7em 0 0}
img{border:none;max-width:100%;width:100%;display:block}
body {font:normal 100% "微软雅黑", Helvetica, Arial, sans-serif;background:#dcdcdc}
/*不同分辨率*/
@media screen and (min-width:240px) {
	body{font-size:37%}
}
@media screen and (min-width:320px) {
	body{font-size:50%}
}
@media screen and (min-width:480px) {
	body{font-size:75%}
}
@media screen and (min-width:640px) {
	body{font-size:100%}
}
@media screen and (min-width:720px) {
	body{font-size:112.5%}
}
@media screen and (min-width:1024px) and (orientation:landscape) {
	body{font-size:100%}
}
@media screen and (min-width:1080px) {
	body{font-size:140%}
}
/*结构*/
#cntbox{display:none}
/*便捷菜单*/
.quick{width:92%;height:auto;background:#f2f2f2 url(/media/images/wifi/block_b_bg.png) repeat-x 0 bottom;background-size:1em 1em;padding:0 4% 0.5em;overflow:auto;zoom:1;margin-bottom:0.75em;border-top:1px solid #f5f5f5}
	.quick li{display:inline-block;float:left;width:25%;text-align:center}
	.quick a{display:block;padding:15%}
	.quick .title{display:block;font-size:1.5em;display:block;margin-top:0.2em;}
.huibox{width:100%;;background:#f2f2f2 url(/media/images/wifi/block_t_bg.png) repeat-x 0 top;background-size:1em 1em;padding-bottom:11em}
	.huibox h3{font-size:1.5em;padding-bottom:0.5em}
	.huigroup{padding-bottom:3em;background:url(/media/images/wifi/loader.gif) no-repeat center bottom;background-size:1.5em}
	.huiblock{width:95%;border-top:1px solid #e6e6e6;border-bottom:0.5em solid #e6e6e6;padding:2% 3% 2% 2%;overflow:auto}
	.imgb{float:left;width:25%}
	.tlt{float:right;width:73%}
		.tlt h4{padding:0.2em 0}
			.tlt h4 a{font-size:2em}
				.tlt h4 span{float:right;font-size:0.75em;color:#d8d8d8;padding-top:0.25em;}
		p.exp{color:#999;font-size:1.5em;width:100%;padding-top:0.15em;height:3em}
		div.moreinfo{font-size:1.3em;color:#666}
/*底部导航*/
#topbar{position:fixed;bottom:-9.5em;height:9.5em;width:100%;opacity:0.97}
	.logobox{height:2.5em;text-align:center;background:url(/media/images/wifi/logobox_bg.png) no-repeat center;background-size:auto 100%}
		.mohelogo{display:inline-block;height:100%;width:14.5em;background:url(/media/images/wifi/mohelogo.png) no-repeat;background-size:cover;overflow:hidden}
		.mohelogo.on{background:none}
	.footnav{height:6em;line-height:6em;width:100%;background:#fff;padding:0.5em 0}
		.fnav{display:inline-block;width:30%;float:left;text-align:center;position:relative}
		.fnav.fnav2{width:40%}
			.fnav a.l1{font-size:2em;display:inline-block;height:100%;width:100%;color:#555}
			.fnav a.l1.on{color:#2ca6e0}
			.fnav .l1.loginbtn{background:#3FA9F5;font-size:2.5em;color:#fff;border-radius:2em}
			.subfnav{position:absolute;left:0;bottom:7em;width:90%;padding:0 5% 1em;background:url(/media/images/wifi/subfnav_bg.png) no-repeat center bottom;background-size: 2em 1em;display:none}
				.subfnav ul{background:#fff;border-radius:0.5em;padding:5% 14%}
				.subfnav li{line-height:4em;border-bottom:1px solid #eee}
				.subfnav li:last-child{border-bottom:none}
				.subfnav li a{font-size:1.75em}
	.loginbox{height:27em;background:#fff}
		.logintype{padding:1.2em 5% 0;overflow:auto}
		.logintype li{float:left;width:40%;padding:0 5%;margin:0 0 5%;line-height:4em}
		.logintype li a{width:100%;border:1px solid #ccc;border-radius:0.5em; display:-webkit-box;display:-moz-box;display:-ms-flexbox;display:-webkit-flex;display:flex}
			.logintype li .lg_ph{border-color:#ffcc33}
			.logintype li .lg_qq{border-color:#aadeec}
			.logintype li .lg_wb{border-color:#fd8390}
			.logintype li .lg_wx{border-color:#8fd678}
			.logintype li .lg_yj{border-color:#d6b572}
		.logintype img{height:4em;width:4em;border-top-left-radius:.3em;border-bottom-left-radius:.3em}
		.logintype span{display:block;font-size:1.75em;padding-left:0.2em;text-align:center; -webkit-box-flex:1;-moz-box-flex:1;-webkit-flex:1;-ms-flex:1;flex:1}
		.loginbox h4{text-align:center;color:#ddd}
		
		
		
		
.qp_lodediv img { display: inline; vertical-align: middle;width: auto;}
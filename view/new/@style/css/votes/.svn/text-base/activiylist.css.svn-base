@charset "utf-8";
/* CSS Document */
body{
	background-color:#f0f0f0;
}
.activity_ad img{
	width:100%;
}
.activity_auto{
	margin-left:auto;
	margin-right:auto;
	width:98%;
}
.activity_search{
	margin-top:5px;
	height:35px;
	line-height:35px;
	border:#e9e9e9 1px solid;
	border-radius:3px;
	box-shadow: 1px 1px 3px #d1d1d1;
	overflow:hidden;
	-webkit-appearance: none;
    -moz-appearance: none;
    background-color:#fff;
}
.activity_search input[type='text']{
height:35px;
	vertical-align:top;
	padding-left:1%;
	width:90%;
	border:none;
	margin-top:0px;
	font-size:14px;
}
.activity_search .activitybtn{
	width:9%;
	height:18px;
	border:none;
	cursor:pointer;
	margin-top:8px;
	background:url(/res/votes/search.png) center center no-repeat;
	background-size:contain;

}
.activity_list{
	margin-top:6px;
}
.activity_list > li{
	float:left;
	width:32%;
	margin-left:2%;
	margin-bottom:5px;
	background-color:#fff;
}
.activity_list > li:nth-child(3n+1){
	margin-left:0%;
}
.activity_list > li > a{
	border:#c0c3c7 1px solid;
	display:block;
	text-decoration:none;
	overflow:hidden;
}
.activity_list > li table{
	width:100%;
	margin-top:2px;
	margin-bottom:2px;
}
.activity_list > li td{
	height:80px;
	text-align:center;
	vertical-align:middle; 
}
.activity_list > li td img{
	height:100%;
}
.activity_list > li h2{
	border-top:#c0c3c7 1px solid;
	padding-left:8px;
	padding-right:8px;
	line-height:21px;
	font-weight:bold;
	font-size:14px;
	padding-top:3px;
}
.activity_list > li span{
	padding-left:8px;
	padding-right:8px;
	color:#ea5504;
	font-weight:bold;
	line-height:19px;
	margin-bottom:2px;
}

/*иом│*/
.loading{position:fixed;top:50%;left:0;right:0;z-index:12;display:none;}
.loading p{width:100%;font-size:1.2em;font-weight:400;color:#fff;text-align:center;}
.loadmore {background:#dddddd;color:#303030;font-size:16px;padding:5px 10px;text-align:center;margin:10px 5px;display:none;-ms-border-radius: 5px;border-radius: 5px;}
.pageloading{display:none;position:fixed;bottom:10px;right:10px;}
.page{color:#FFF;background:#333;font-size:14px;font-weight:bold;float:right;padding:5px;} 

/*foot*/
.activity_foot{
	border:#adaeb1 1px solid;
	background-color:#e9e9e9;
	z-index:999;
	position:fixed;
	left:0;
	bottom:0;
	width:100%;
}
.activity_foot li{ float:left; width:25%; display: inline-block;}
.activity_foot a, .activity_foot span{
	display:block;
	text-align:center;
	background-position:center top;
	background-repeat:no-repeat;
	background-size:auto 50%;
	height:20px;
	padding-top:25px;
	margin-top:6px;
	margin-bottom:6px;
	vertical-align:bottom;
	font-size:14px;
    text-decoration:none;
}

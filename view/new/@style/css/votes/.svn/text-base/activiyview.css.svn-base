@charset "utf-8";
/* CSS Document */
body{ font-size:12px;}
.activiy_view{
	width:100%;
	margin-left:auto;
	margin-right:auto;
	margin-top:12px;
}
.toptu{ padding:0px 3% 8px; width:94%; border-bottom:3px solid #3773e1;}
.omiss{white-space:normal;}

.shoujiang{ width:98%; margin:5px auto;}
.shoujiang a{ border:1px solid #D47815;;background: -webkit-gradient(linear, 0 0, 0 100%, from(#FD9C0D), to(#F36A0C)); font-size:14px; color:#FFFFFF; font-family:'微软雅黑';border-radius:5px; display:block; text-align:center; padding:8px 0px;}
.toptu > h2{
padding:5px 0px 10px 0px;
	font-size:16px;
	font-weight:bold;
	line-height:20px; text-align:center; width:80%; margin:auto;
}
.activiy_view > h2,.activiy_rule h2{
	font-size:16px;
	font-weight:bold;
	line-height:28px;
}
.activiy_view > span{
	color:#727171;
	font-size:14px;
}
.activity_view{
	width:97%;
	margin-top:10px;
	padding-left:3%;
}


.activity_view > div:nth-child(1){
	width:46%;
}
.activity_view > div:nth-child(2){
	margin-left:21px;
}
.activity_view > div:nth-child(2) > p{
	line-height:23px;
	font-size:15px;
}
.activity_view > div:nth-child(2) > p > span{
	color:#fb6600;
	font-weight:bold;
}
.activity_view > div:nth-child(2) > div{
	background-color:#fb6600;
	height:31px;
	line-height:31px;
	border-radius:5px;
	width:120px;
	text-align:center;
	color:#fff;
	font-size:16px;
	font-weight:bold;
	cursor:pointer;
	margin-top:3px;
}
.sjztp{
	width:100%;
	overflow:hidden;
	border:#b0b1b1 1px solid;
}
.sjztp table{
	width:100%;
}
.sjztp td{
	height:100px;
	text-align:center;
	vertical-align:middle;
	overflow:hidden;
}
.sjztp img{ height:100px;vertical-align:middle;overflow:hidden;}
.photoshow{
	border:#b0b1b1 1px solid;

	overflow:hidden;
}
.photoshow table{
	width:100%;
	margin-top:2px;
	margin-bottom:2px;
}
.photoshow td{
	height:120px;
	text-align:center;
	vertical-align:middle;
	overflow:hidden;
}
.photoshow img{height:120px;padding:0px; margin:0px; border:0px; vertical-align:middle;overflow:hidden;}
.photomark{
	font-weight:bold;
	font-size:15px;
	line-height:36px; 
	height:36px; 
	overflow:hidden; 
	width:98%;
}
.photomarka{
	font-weight:bold;
	font-size:15px;
	line-height:36px; height:36px; overflow:hidden; width:100px;
}
.photolist{ width:94%; margin:auto; padding-top:5px;}
.photolist > li{
	float:left;
	width:47%;
	overflow:hidden;
}
.photolist > li:nth-child(odd){
	margin-right:3%;
}
.photolist > li:nth-child(even){
	margin-left:3%;
}
.activity_set{
margin-top:10px;
border-top:5px solid #3773e1;
line-height:30px;
}
.activity_set table,.rule_content{
	width:94%;
	margin-left:auto;
	margin-right:auto;
	padding:10px 0px;
}
.activity_set td:nth-child(1){
    background:url(../images/jiangpinbt.png) no-repeat left center; padding-left:20px; background-size:12px 20px;
	font-weight:bold;
	font-size:16px;
}
.activity_set td:nth-child(2){
	text-align:center;
}
.activity_set td:nth-child(2)> div{ width:90%; margin:auto; border:1px solid #999999; border-radius: 5px;-moz-border-radius: 5px}
.activity_set td:nth-child(2) a{
display:block;
	background:url(../images/phone.png) 5px center no-repeat;
	background-size:15px auto;
	font-size:14px; padding-left:20px;
}
.activity_set td:nth-child(3){
	text-align:right;
}
.activity_set td:nth-child(3) a{
	font-size:14px;
	color:#5f9bf8;
	text-decoration:underline;
}
.prize_item{
}
.prize_item .tip{
	width:94%;
	height:34px;
	margin-left:auto;
	margin-right:auto;
}
.prize_item .tip > div:nth-child(1){
	line-height:34px;
	font-size:14px;
	font-weight:bold;
}

.prize_item > div:nth-child(1){
	line-height:34px;
	font-size:14px;
	font-weight:bold;
}
.prize_item > div span:nth-child(1){float:left;}
.prize_item > div span:nth-child(2){color:#3d4245;float:right;}
.prize_item > div span:nth-child(2) a{ background: url(../Images/tptel.png) no-repeat right center; background-size:20px auto;}
.prize_item > div:nth-child(1) span:nth-child(1){color:#e60012}
.prize_item > div:nth-child(3) span:nth-child(1){color:#ea5504}
.prize_item > div:nth-child(5) span:nth-child(1){color:#009944}


.prize_item{width:94%;margin-left:auto;margin-right:auto;}
.prize_item .photo{border:1px solid #9fa0a0; padding:1px; }
.activity_share{
	width:96%;
	margin-left:auto;
	margin-right:auto;
}
.prize_item .photo img{
	width:100%;vertical-align:middle;overflow:hidden;
}

.share{
	width:100%;
}
.share > a{
	border:#3d4245 1px solid;
	display:block;
	text-decoration:none;
	text-align:center;
	margin-left:2.5%;
	margin-right:2.5%;
	margin-top:10px;
	margin-bottom:10px;
	height:28px;
	line-height:28px;
	border-radius:6px;
	cursor:pointer;
}
.prize_list{ width:100%; margin:auto;}
div.prize_list > div:nth-child(1) .tip > div:nth-child(1){
	color:#e60012;
}
div.prize_list > div:nth-child(2) .tip > div:nth-child(1){
	color:#ef8043;
}
div.prize_list > div:nth-child(3) .tip > div:nth-child(1){
	color:#40b373;
}

.activity_share{
	margin-top:3px;
}
.activity_share > a{
	height:35px;
	border:#d1d2d6 1px solid;
	line-height:35px;
	margin-right:6px;
	background-color:#f7f7f7;
	font-size:14px;
	display:block;
	text-decoration:none;
}
.activity_share a:nth-child(1),.activity_share a:nth-child(2){
	background-position:21px center;
	background-repeat:no-repeat;
	background-size:auto 50%;
	padding-left:48px;
}
.activity_share a:nth-child(1){
	background-image:url(../images/weixin.png);
}
.activity_share a:nth-child(2){
	background-image:url(../images/weibo.png);
}
.activity_share a:nth-child(3){
	margin-right:0px;
	width:55px;
	text-align:center;
}
/*rule*/
.activiy_rule{
	border-bottom:#c0c3c7 1px solid;
	margin-top:10px;
}
.activiy_rule h2,.activiy_rule h3{
	margin-left:auto;
	margin-right:auto;
	width:94%;
}
.activiy_rule h3{
	color:#86888e;
	font-size:14px;
	margin-bottom:6px;
	line-height:21px;
}
.rule_content{
	line-height:23px;
	margin-top:8px;
}

.baoming{ background:url(/res/votes/baoming.jpg) no-repeat center center; background-size:100% auto; width:94%; margin:5px auto; height:50px; line-height:50px; text-align:center; color:#FFFFFF; font-size:16px; color:#FFFFFF}
.baoming a{ display:block; color:#FFFFFF;}
.baoming span:nth-child(1){ text-decoration:underline;}
.baoming span:nth-child(2){ color:#f6ff00}


.prize_item { width:96%; margin:auto;}
.prize_item > div > p{color:#3d4245;}
.prize_list > div:nth-child(1) > div > p:nth-child(1){color:#e60012}
.prize_list > div:nth-child(2) > div > p:nth-child(1){color:#ea5504}
.prize_list > div:nth-child(3) > div > p:nth-child(1){color:#009944}
.prize_item > div > div { border:1px solid #9fa0a0; border-top:0px;} 
.prize_item > div > div > div:last-child {padding:2%; width:96%; display:block; overflow:hidden;}


.prize_item > div > div > div:last-child{clear:both;font-family:"Microsoft YaHei",Heiti SC,Helvetica,Arial,sans-serif!important;background:none!important; height:auto!important;} 
.prize_item > div > div > div:last-child img{ width:100%!important; height:auto!important;}
.prize_item > div > div > div:last-child span{font-family:"Microsoft YaHei",Heiti SC,Helvetica,Arial,sans-serif!important;}
.prize_item > div > div > div:last-child ul{ width:100%!important; float:none!important;font-family:"Microsoft YaHei",Heiti SC,Helvetica,Arial,sans-serif!important; background:none!important;}
.prize_item > div > div > div:last-child ul > li:last-child{ float:none!important; clear:both!important;}
.prize_item > div > div > div:last-child ul > li{font-size:13px!important; font-weight:normal!important;}
.prize_item > div > div > div:last-child p{font-family:"Microsoft YaHei",Heiti SC,Helvetica,Arial,sans-serif!important;background:none!important; height:auto!important;}

.prize_itema{font-size:14px;height:34px; line-height:34px; border:1px solid #9fa0a0; border-bottom:0px; margin-top:10px;}

.prize_itema p:nth-child(1){ float:left; padding-left:2%;}
.prize_itema p:nth-child(2){ float:right; padding-right:1%;}
.prize_itema p:nth-child(2) a{ background: url(../Images/phone.png) no-repeat left 12px;background-size:12px auto; padding-left:17px;line-height:34px; display:block;}
.sharea{width:96%; padding:10px 0px; height:28px; line-height:28px;}
.sharea p{width:50%; text-align:center; float:left; background:none;} 
.sharea p a{width:90%; margin:auto;border:#3d4245 1px solid;display:block;text-decoration:none;text-align:center;height:28px;line-height:28px;border-radius:6px;cursor:pointer;}
/*foot*/
.activity_foot{
	border:#adaeb1 1px solid;
	background-color:#e9e9e9;
	z-index:999;
	position:fixed;
	left:0;
	bottom:0;
	width:100%;
}
.activity_foot li{ float:left; width:25%;}
.activity_foot a{
	display:block;
	text-align:center;
	background-position:center top;
	background-repeat:no-repeat;
	background-size:auto 50%;
	height:20px;
	padding-top:25px;
	margin-top:6px;
	margin-bottom:6px;
	vertical-align:bottom;
	font-size:14px;
    text-decoration:none;
}
.activity_foot li:nth-child(1) a{
	background-image:url(../images/book.png);
}
.activity_foot li:nth-child(2) a{
	background-image:url(../images/pen.png);
}
.activity_foot li:nth-child(3) a{
	background-image:url(../images/manage.png);
}
.activity_foot li:nth-child(4) a{
	background-image:url(../images/arc.png);
}
.fixshowed{
	height:65px;
}
.sign_content,.code_content {
    height: auto;
    position: fixed;
    width: 90%;
    left:5%;
    z-index: 3;
    top: 10%;
    overflow: hidden;
    display:none;
}
.guide_content {
    display:none;
    height: auto;
    right: 2%;
    position: fixed;
    top: 15%;
    width: auto;
    z-index: 3;
    text-align:center;
}
.load_content {
    display:none;
    height: auto;
    position: fixed;
    width: auto;
    z-index: 3;
    top: 50%;
    left:50%;
    text-align:center;
    background-color:#fff;
	margin-left:-11px;
	*margin-left:-11px;
	_margin-left:-11px;
}

.recommendlist{ width:96%; margin:auto;}
.recommendlist li{ margin-top:6px;}
.recommendlist li:nth-child(1){ margin:0px;}
.recommendlist li img{ width:100%;}
#lean_overlay {
    position: fixed;
    z-index: 100;
    top: 0px;
    left: 0px;
    height: 100%;
    width: 100%;
    background: #000;
    display: none;
    opacity: 0.5;
}
#itemtip {
    padding: 10px;
    width:60%;
    display: none;
    background: #FFF;
    border-radius: 5px;
    -moz-border-radius: 5px;
    -webkit-border-radius: 5px;
    box-shadow: 0px 0px 4px rgba(0,0,0,0.7);
    -webkit-box-shadow: 0 0 4px rgba(0,0,0,0.7);
    -moz-box-shadow: 0 0px 4px rgba(0,0,0,0.7);
}
.modal_close {
    position: absolute;
    top: 12px;
    right: 12px;
    display: block;
    width: 14px;
    height: 14px;
    background: url(/Scripts/jquery.leanModa/images/modal_close.png);
    z-index: 2;
    background-size: contain;
    cursor:pointer;
}